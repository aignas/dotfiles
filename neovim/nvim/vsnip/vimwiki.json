{
  "tags": {
    "prefix": "tags",
    "scope": "vimwiki",
    "body": "---\ntags: [$0]\n---\n",
    "description": "Tags for a vimwiki entry"
  },
  "td": {
    "prefix": "td",
    "scope": "vimwiki",
    "body": "# $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE: $1\n$0",
    "description": "Heading for the diary entry"
  },
  "[]": {
    "prefix": "[]",
    "scope": "vimwiki",
    "body": "[$1]($2.md)$0",
    "description": "A link to a zetel"
  },
  "[w": {
    "prefix": "[w",
    "scope": "vimwiki",
    "body": "[$1](wn.work:$2.md)$0",
    "description": "Crosslink to work zettelkasten"
  },
  "[p": {
    "prefix": "[p",
    "scope": "vimwiki",
    "body": "[$1](wn.personal:$2.md)$0",
    "description": "Crosslink to personal zettelkasten"
  }
}
