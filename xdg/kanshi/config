profile {
    output eDP-1 disable
    output "Dell Inc. DELL U2421E JFHVC23" mode 1920x1200 position 0,0
}

profile {
    output eDP-1 disable
    output "Dell Inc. DELL U2723QX HS5M4P3" mode 3840x2160 position 0,0 scale 1.3
}

profile {
    output eDP-1 enable mode 1920x1080
}

profile {
    output "Dell Inc. DELL U2421E JFHVC23" enable mode 1920x1200 position 0,0
}
