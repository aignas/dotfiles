fn PROMPT
    let path="${c::blue}${PWD}${c::default}"
    let symbol="${c::magenta}‚ùØ${c::default}"
    let git=$(git rev-parse --abbrev-ref HEAD)
    echo -en "${path} ${c::242}${git}${c::default} \n${symbol} "
end

export EDITOR=nvim

alias g=git
alias x=exa
alias vc=nvim ~/.config/nvim/init.vim
alias xx="exa -l"
alias e="nvim"
alias r="ranger"

